{
  "name": "ci-cd-no-cli",
  "version": "1.0.0",
  "description": "CI/CI example without using framework CLI",
  "author": "Mohamed Sallam",
  "license": "MIT",
  "main": "",
  "bugs": {
    "url": "https://github.com/sallamx99/ci-cd-no-cli/issues"
  },
  "homepage": "https://github.com/sallamx99/ci-cd-no-cli#readme",
  "keywords": [
    "ci",
    "cd",
    "google-cloud",
    "google-cloud-build"
  ],
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "serve": "rm -rf ./dist && parcel serve src/index.html",
    "serve:e2e": "node ./scripts/cypress.js",
    "bundle": "rm -rf ./dist && parcel build src/index.html --no-source-maps",
    "lint:code": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "lint:style": "stylelint '**/*.{css,vue}' --ignore-path .gitignore",
    "lint": "npm run lint:code && npm run lint:style",
    "dups": "jsinspect --threshold 30 --minimum-instances 2 src",
    "test:e2e": "node ./scripts/cypress.js --headless --no-video",
    "test:unit": "mocha --require @babel/register ./test/unit/**/*.unit.js",
    "coverage": "NODE_ENV=test nyc npm run test:unit",
    "build": "npm run lint && npm run dups && npm run coverage && npm run test:e2e && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sallamx99/ci-cd-no-cli.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.5",
    "@babel/register": "^7.4.4",
    "@vue/component-compiler-utils": "^3.0.0",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-istanbul": "^5.1.4",
    "chai": "^4.2.0",
    "cypress": "^3.3.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "jsinspect": "^0.12.7",
    "mocha": "^6.1.4",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.12.3",
    "postcss": "^7.0.17",
    "prettier": "^1.18.2",
    "shelljs": "^0.8.3",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "vue-hot-reload-api": "^2.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@babel/core": "^7.4.5",
    "vue": "^2.6.10",
    "vue-router": "^3.0.6"
  }
}
